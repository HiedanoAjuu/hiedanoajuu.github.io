#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/Documents/blog/node_modules/.store/sw-precache@5.2.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/Documents/blog/node_modules/.store/sw-precache@5.2.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sw-precache@5.2.1/node_modules/sw-precache/cli.js" "$@"
else
  exec node  "$basedir/../../../../../sw-precache@5.2.1/node_modules/sw-precache/cli.js" "$@"
fi
