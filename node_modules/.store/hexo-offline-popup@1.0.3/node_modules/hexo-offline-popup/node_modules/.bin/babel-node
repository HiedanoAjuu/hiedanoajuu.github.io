#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/Documents/blog/node_modules/.store/babel-cli@6.26.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/Documents/blog/node_modules/.store/babel-cli@6.26.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../babel-cli@6.26.0/node_modules/babel-cli/bin/babel-node.js" "$@"
else
  exec node  "$basedir/../../../../../babel-cli@6.26.0/node_modules/babel-cli/bin/babel-node.js" "$@"
fi
