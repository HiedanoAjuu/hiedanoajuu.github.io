#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/Documents/blog/node_modules/.store/flow-remove-types@2.215.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/Documents/blog/node_modules/.store/flow-remove-types@2.215.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../flow-remove-types@2.215.1/node_modules/flow-remove-types/flow-remove-types" "$@"
else
  exec node  "$basedir/../../../../../flow-remove-types@2.215.1/node_modules/flow-remove-types/flow-remove-types" "$@"
fi
