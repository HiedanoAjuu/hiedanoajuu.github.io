#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/Documents/blog/node_modules/.store/npm-path@1.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/Documents/blog/node_modules/.store/npm-path@1.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../npm-path@1.1.0/node_modules/npm-path/bin/npm-path" "$@"
else
  exec node  "$basedir/../../../../../npm-path@1.1.0/node_modules/npm-path/bin/npm-path" "$@"
fi
